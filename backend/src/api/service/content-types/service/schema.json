{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "duration": {
      "type": "integer",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "slug": {
      "type": "uid",
      "required": true,
      "minLength": 3,
      "maxLength": 110
    }
  }
}
